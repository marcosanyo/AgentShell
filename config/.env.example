# Camera settings
CAMERA_IP="YOUR_CAMERA_IP"
CAMERA_PORT="2020"
CAMERA_USER="YOUR_CAMERA_USERNAME"
CAMERA_PASSWORD="YOUR_CAMERA_PASSWORD"

# Google API Key for Generative AI
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY"

# AWS Settings for STT/TTS
AWS_REGION="ap-northeast-1"
# AWS credentials will be automatically loaded from:
# - Environment variables (AWS_ACCESS_KEY_ID, AWS_SECRET_ACCESS_KEY)
# - AWS CLI configuration (~/.aws/credentials)
# - IAM role (when running on EC2/ECS/Lambda)

# Optional: S3 bucket for AWS Transcribe (required for standard API)
# AWS_TRANSCRIBE_S3_BUCKET="your-bucket-name"

# MCP Server URL (if different from default)
# MCP_SERVER_URL="http://127.0.0.1:9006/sse/"
