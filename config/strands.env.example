# AWS Strands Agent Configuration

# AWS Credentials (required for Bedrock access)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_SESSION_TOKEN=your_aws_session_token  # Optional, for temporary credentials
AWS_REGION=us-west-2

# Strands Agent Server Configuration
STRANDS_SERVER_PORT=8001
STRANDS_LOG_LEVEL=INFO

# AWS Bedrock Model Configuration (using cross-region inference profiles)
BEDROCK_MODEL_ID=us.anthropic.claude-3-5-haiku-20241022-v1:0
BEDROCK_TEMPERATURE=0.7
BEDROCK_MAX_TOKENS=2048

# Fallback Models (in order of preference)
BEDROCK_FALLBACK_MODEL_ID=us.anthropic.claude-3-5-sonnet-20241022-v2:0

# Multi-Agent Configuration
DEFAULT_AGENT_ID=camera_agent_01
MAX_AGENTS=5

# MCP Server Integration
MCP_SERVER_URL=http://127.0.0.1:9006/sse/
MCP_CONNECTION_TIMEOUT=30

# Coordination Settings
COORDINATION_ENABLED=true
COORDINATION_LOG_LEVEL=INFO

# Performance Settings
AGENT_RESPONSE_TIMEOUT=60
STREAM_CHUNK_SIZE=1024